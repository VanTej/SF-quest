{% extends 'base.html.twig' %}

{% block body %}
	<h1>Toutes les catégories</h1>
	{% for category in categories %}
		<ul>
			<li> <a href="{{ path('category_show', {'categoryName': category.name}) }}">{{ category.name }} </a></li>
		</ul>
	{% else %}
		Aucune série trouvée.
	{% endfor %}
{% endblock %}

{% extends 'base.html.twig' %}

{% block body %}

	<h1>Ma catégorie {{ category.name }}</h1>

	{% for program in programs %}
		<ul>
			<li>
				<a href="{{ path('program_show', {'id': program.id}) }}">{{ program.title }}</a>
			</li>
		</ul>
	{% else %}
		Aucune série trouvée.
	{% endfor %}


{% endblock %}
